<div id="<%= dom_id book %>" class="card mb-4">
  <div class="card-body">
    <h5 class="card-title"><%= book.title %></h5>
    <p class="card-text">
      <strong>Author:</strong> <%= book.author %>
    </p>
    <p class="card-text">
      <strong>Description:</strong> <%= book.desc %>
    </p>
  </div>
</div>
